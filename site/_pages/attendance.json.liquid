[
    {%- for post in site.categories.relatorios %}
    {
        "name": "{{ post.title }}",
        "attendees": [
            {%- for attendee in post.attendees -%}
            "{{ attendee }}"{% unless forloop.last %},{% endunless %}
            {%- endfor -%}
        ]
    }{% unless forloop.last %},{% endunless %}
    {%- endfor %}
]
